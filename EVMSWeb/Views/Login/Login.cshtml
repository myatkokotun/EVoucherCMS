    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="~/vendor/font-awesome/css/all.min.css">
    <link rel="stylesheet" href="~/vendor/hs-mega-menu/dist/hs-mega-menu.min.css">

    <!-- CSS Front Template -->
    <link rel="stylesheet" href="~/css/theme.css">
</head>
<body>
    <!-- ========== HEADER ========== -->
    <header id="header" class="header">

        <div class="header-section">
            <div id="logoAndNav" class="container">
                <!-- Nav -->
                <nav class="js-mega-menu navbar navbar-expand-lg">
                    <!-- Logo -->
                    <a class="navbar-brand" href="~/Home/index.html" aria-label="Front">
                        @*<img src="~/svg/logos/logo.svg" alt="Logo">*@
                        <img src="~/image/shoppinglogo.png" style="width:60%" />
                    </a>
                    <!-- End Logo -->
                    <!-- Navigation -->
                    <div id="navBar" class="collapse navbar-collapse">
                        <ul class="navbar-nav">
                            <!-- Home -->
                            <li class="hs-has-mega-menu navbar-nav-item">
                                <a id="homeMegaMenu" class="hs-mega-menu-invoker nav-link" href="javascript:;" aria-haspopup="true" aria-expanded="false">About</a>
                            </li>
                            <li class="hs-has-mega-menu navbar-nav-item">
                                <a id="homeMegaMenu" class="hs-mega-menu-invoker nav-link" href="javascript:;" aria-haspopup="true" aria-expanded="false">Contact</a>
                            </li>
                            <li class="hs-has-mega-menu navbar-nav-item">
                                <a id="homeMegaMenu" class="hs-mega-menu-invoker nav-link" href="javascript:;" aria-haspopup="true" aria-expanded="false">Help</a>
                            </li>
                        </ul>
                    </div>
                    <!-- End Navigation -->
                </nav>
                <!-- End Nav -->
            </div>
        </div>
    </header>
    <!-- ========== END HEADER ========== -->
    <!-- ========== MAIN ========== -->
    <main id="content" role="main">
        <!-- Login Form -->
            <div class="container space-2 space-lg-1">
                <form class="js-validate w-md-75 w-lg-50 mx-md-auto" class="form" id="LoginForm" method="post" asp-action="Login">
                    <!-- Title -->
                    <div class="mb-5 mb-md-7">
                        <h3 class="h2 mb-0">Welcome to Evoucher Management System</h3>
                        <p>Login to your account.</p>
                    </div>
                    <!-- End Title -->
                    <!-- Form Group -->
                    <div class="js-form-message form-group">
                        <label class="input-label" for="signinUsername">Username</label>
                        <input type="text" class="form-control" name="username" id="signinUsername" asp-for="Username" placeholder="Username" aria-label="Username" required
                               data-msg="Please enter a valid username.">
                    </div>
                    <!-- End Form Group -->
                    <!-- Form Group -->
                    <div class="js-form-message form-group">
                        <label class="input-label" for="signinSrPassword">
                            <span class="d-flex justify-content-between align-items-center">
                                Password
                                <a class="link-underline text-capitalize font-weight-normal" href="recover-account-simple.html">Forgot Password?</a>
                            </span>
                        </label>
                        <input type="password" class="form-control" name="password" id="signinSrPassword" asp-for="Password" placeholder="********" aria-label="********" required
                               data-msg="Your password is invalid. Please try again.">
                    </div>
                    <!-- End Form Group -->
                    @{
                        if (ViewBag.Unauthorize == "Unauthorize")
                        {
                            <div class="form-group">
                                <label class="text-danger font-weight-semibold">Wrong email or password !</label>
                            </div>
                        }
                    }
                    <!-- Button -->
                    <div class="row align-items-center mb-5">
                        <div class="col-sm-6 mb-3 mb-sm-0">
                            <span class="font-size-1 text-muted">Don't have an account?</span>
                            <a class="font-size-1 font-weight-bold" href="#">Contact admin</a>
                        </div>

                        <div class="col-sm-6 text-sm-right">
                            <button type="submit" class="btn btn-primary transition-3d-hover">Get Started</button>
                        </div>
                    </div>
                    <!-- End Button -->
                </form>
            </div>
        <!-- End Login Form -->
    </main>
    <!-- ========== END MAIN ========== -->
    <!-- ========== FOOTER ========== -->
    <footer class="bg-navy">
        <div class="container">
            <div class="space-top-2 space-bottom-1 space-bottom-lg-2">
                <div class="row justify-content-lg-between">
                    <div class="col-lg-3 ml-lg-auto mb-5 mb-lg-0">
                        <!-- Nav Link -->
                        <ul class="nav nav-sm nav-x-0 nav-white flex-column">
                            <li class="nav-item">
                                <a class="nav-link media" href="javascript:;">
                                    <span class="media">
                                        <span class="fas fa-location-arrow mt-1 mr-2"></span>
                                        <span class="media-body">
                                            153 Yuzana Plaza, Mingaladon
                                        </span>
                                    </span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link media" href="tel:1-062-109-9222">
                                    <span class="media">
                                        <span class="fas fa-phone-alt mt-1 mr-2"></span>
                                        <span class="media-body">
                                            +95 5792321537
